<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Canal de notificação padrão para Firebase Cloud Messaging -->
    <string name="default_notification_channel_id" translatable="false">mindcare_default_channel</string>

    <!-- Tema aplicado à janela do Android enquanto o processo está iniciando, quando o modo escuro do sistema operacional está desativado -->
    <style name="LaunchTheme" parent="@android:style/Theme.Light.NoTitleBar">
        <!-- Exibe uma tela de splash na atividade. Removida automaticamente quando
             o motor do Flutter desenha seu primeiro quadro -->
        <item name="android:windowBackground">@drawable/launch_background</item>
    </style>

    <!-- Tema aplicado à janela do Android assim que o processo for iniciado.
         Este tema determina a cor da janela do Android enquanto a UI do Flutter é inicializada,
         bem como o plano de fundo atrás da UI do Flutter enquanto está em execução.

         Este tema é usado apenas a partir da V2 da estrutura de embedding do Flutter para Android. -->
    <style name="NormalTheme" parent="@android:style/Theme.Light.NoTitleBar">
        <item name="android:windowBackground">?android:colorBackground</item>
    </style>
</resources>
